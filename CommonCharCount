def commonCharacterCount3(s1, s2):
    count = 0
    commons = set(s1) & set(s2)
    for i in commons:
        count += min(s1.count(i), s2.count(i))
    print(count)
    print(set(s1))
    print(set(s2))
    print(commons)
    print(s1.count(i))
    print(s2.count(i))





s1="aabcc"
s2="adcaa"
commonCharacterCount3(s1,s2)
