
def areSimilar(a,b):
    s= len(b)/2
    if a == b:
        print('True')
    else:
        for i in range(0,len(b)-1):
            b[i],b[i+1] = b[i+1],b[i]
            if a!=b:
                print('False')
                break
        """if a != b:
            print('False')"""


a= [2, 3, 1]
b= [1, 3, 2]
areSimilar(a,b)
